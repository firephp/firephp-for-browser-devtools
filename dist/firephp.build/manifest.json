{
    "manifest_version": 2,
    "description": "(NOTE: This is a PREVIEW build for branch: pre) Log from PHP to a devtools panel.",
    "name": "FirePHP (Official) [pre]",
    "version": "1.4.0",
    "applications": {
        "gecko": {
            "id": "FirePHPExtension-Build_branch_pre@firephp.org",
            "strict_min_version": "56.0"
        }
    },
    "icons": {
        "48": "skin/__icon_48.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "webRequest",
        "webNavigation",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "content_security_policy": "script-src 'self'; style-src 'self'; object-src 'self'; img-src 'self'",
    "devtools_page": "devtools-page.html",
    "background": {
        "page": "background.html"
    },
    "version_name": "1.4.0 pre"
}